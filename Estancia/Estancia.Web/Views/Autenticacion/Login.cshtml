@{
    ViewData["Title"] = "Login";
}

<div class="text-center">
    <h1 class="display-4">Login</h1>
    <form method="post">
        <div class="form-group">
            <label for="Mail">Mail</label>
            <input autocomplete="Mail" name="Mail" type="email" class="form-control" id="Mail"
                placeholder="Ingresa Mail">
        </div>
        <div class="form-group">
            <label for="Contrasena">Contrasena</label>
            <input autocomplete="Contrasena" name="Contrasena" type="password" class="form-control" id="Contrasena"
                placeholder="Ingresa Contrasena">
        </div>

        <button type="submit" class="btn btn-primary">Login</button>
    </form>
</div>
